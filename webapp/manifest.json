{
	"_version": "1.8.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.olympus.apps.edi",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.2.2"
		},
		"ach": "ach",
		"resources": "resources.json",
		"dataSources": {
			"ZEDI_COB_SRV": {
				"uri": "/sap/opu/odata/sap/ZEDI_COB_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZEDI_COB_SRV/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		}
	},
	"sap.fiori": {
		"_version": "1.1.0",
		"registrationIds": [],
		"archeType": "transactional"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"_version": "1.3.0",
		"rootView": {
			"viewName": "com.olympus.apps.edi.view.App",
			"type": "XML",
			"async": false,
			"id": "app"
		},
		"autoPrefixId": true,
		"dependencies": {
			"minUI5Version": "1.50.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"": {
				"uri": "/here/goes/your/serviceurl/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZEDI_COB_SRV",
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.olympus.apps.edi.view",
				"controlId": "idAppControl",
				"clearTarget": false,
				"controlAggregation": "detailPages",
				"async": false,
				"bypassed": {
					"target": ["OneCustomer", "EdiCustomerOnbording"]
				}
			},
			"targets": {
				"EdiCustomerOnbording": {
					"controlAggregation": "masterPages",
					"viewName": "EdiCustomerOnbording",
					"viewId": "EdiCustomerOnbording",
					"viewLevel": 1,
					"transition": "slide"
				},
				"OneCustomer": {
					"controlAggregation": "detailPages",
					"viewName": "OneCustomer",
					"viewId": "OneCustomer",
					"viewLevel": 1,
					"transition": "slide"
				},
				"Mass": {
					"controlAggregation": "detailPages",
					"viewName": "Mass",
					"viewId": "Mass",
					"viewLevel": 1,
					"transition": "slide"
				}
			},
			"routes": [{
				"pattern": "EdiCustomerOnbording",
				"name": "EdiCustomerOnbording",
				"target": ["OneCustomer", "EdiCustomerOnbording"]
			}, {
				"pattern": "EdiCustomerOnbording/OneCustomer",
				"name": "OneCustomer",
				"target": ["EdiCustomerOnbording", "OneCustomer"]
			}, {
				"pattern": "EdiCustomerOnbording/Mass",
				"name": "Mass",
				"target": ["EdiCustomerOnbording", "Mass"]
			}, {
				"pattern": "",
				"name": "default",
				"target": ["OneCustomer", "EdiCustomerOnbording"]
			}]
		}
	}
}